{
  "name": "v-museum",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:debug": "vue-cli-service test:unit --inspect-brk --watch",
    "lint": "vue-cli-service lint",
    "start": "npm run serve"
  },
  "dependencies": {
    "@element-plus/theme-chalk": "^2.2.16",
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "cors": "^2.8.5",
    "element-plus": "1.1.0-beta.22",
    "openseadragon": "^2.4.2",
    "smoothscroll-polyfill": "^0.4.4",
    "vue": "^3.2.19",
    "vue-router": "^4.0.11",
    "vue-window-size": "^1.1.2",
    "vuex": "^4.0.2",
    "vuex-persist": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-unit-mocha": "^4.5.13",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/test-utils": "^2.0.0-0",
    "babel-eslint": "^10.1.0",
    "chai": "^4.1.2",
    "cypress": "^8.2.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0",
    "sinon": "^11.1.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "mocha": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
